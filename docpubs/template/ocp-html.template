$if(toc)$
<p id="toc-header">CONTENTS</p>
<nav class="toc">
$table-of-contents$
</nav>
$endif$
<article class="markdown-body">
$body$
</article>
$for(include-after)$
$include-after$
$endfor$
</body>
</html>
